    "clean:tarball": "node_modules/.bin/rimraf stateli*.tgz"
  },
  "devDependencies": {
    "@types/jest": "^25.1.4",
    "coveralls": "^3.0.9",
    "jest": "^25.1.0",
    "rimraf": "^3.0.2",
    "tslint": "^6.1.0",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "rxjs": "^6.5.4"
  }
}
