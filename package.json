{
  "name": "stateli",
  "version": "1.0.1",
  "homepage": "",
  "jest": {},
  "main": "dist/index.js",
  "scripts": {
    "start": "npm run build && node dist/index.js",
    "build": "npm run clean:dist && tsc",
    "prepack": "npm run clean:tarball && npm run build",
